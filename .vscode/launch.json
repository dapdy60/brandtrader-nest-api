{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "launch",
        "name": "Debug Nest App",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run",
          "start:debug"
        ],
        "restart": true,
        "timeout": 60000,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Run Tests",
        "runtimeExecutable": "npm",
        "runtimeArgs": [
          "run",
          "test"
        ],
        "restart": false,
        "timeout": 60000,
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen"
      }
    ]
  }
  